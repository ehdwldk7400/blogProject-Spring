<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.jin.dao.TagDAO"> 
  	<insert id="createTag">
  		insert into tag(tagname) values(#{tagname});
  	</insert>
  	<select id="tagList" resultType="com.jin.doamin.tagVO">
  		select * from tag
  	</select>
  	
  </mapper>